    "istanbul": "istanbul",
    "lint": "eslint --ext .js --ext .jsx . && stylelint -q './src/**/*.css'",
    "mocha": "mocha",
    "start": "webpack-dev-server --config webpack/webpack.dev.config.js --progress",
    "stylelint": "stylelint",
    "test": "istanbul cover _mocha -- './src/**/*.test.{js,jsx}'",
    "webpack": "webpack",
