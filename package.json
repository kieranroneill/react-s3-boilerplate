{
  "name": "react-s3-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Boilerplate code for building a frontend React application that is statically served from an S3 bucket.",
  "main": "src/index.jsx",
  "repository": {
    "type": "git",
